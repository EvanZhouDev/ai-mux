{
	"name": "ai-mux",
	"version": "1.1.0",
	"description": "Strategically alternate between various Vercel AI SDK language models",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"files": [
		"dist"
	],
	"sideEffects": false,
	"scripts": {
		"build": "tsup src/index.ts --format esm --dts --outDir dist --clean",
		"prepublishOnly": "npm run build",
		"test": "vitest run",
		"test:watch": "vitest --watch"
	},
	"engines": {
		"node": ">=18"
	},
	"devDependencies": {
		"@types/node": "^24.10.1",
		"ai": "6.0.0-beta.138",
		"tsup": "^8.5.1",
		"typescript": "^5.9.3",
		"vitest": "latest"
	},
	"peerDependencies": {
		"ai": "6.0.0-beta.138"
	}
}
